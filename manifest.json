{
    "manifest_version": 2,
    "name": "FreeKinopoisk",
    "version": "1.0",
    "description": "Смотри любые фильмы на кинопоиске бесплатно!",
    "icons": {
        "256": "256.png"
    },
    "content_scripts": [
        {
            "matches": ["https://www.kinopoisk.ru/film/*"],
            "js": ["content_script.js"]
        }
    ],
    "permissions": [
        "activeTab"
    ]
}
